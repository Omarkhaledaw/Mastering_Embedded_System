#include <stdio.h>

int main(){
	char ch;
	printf("Enter a character: ");
	fflush(stdout);
	scanf("%c",&ch);
	if(((ch >= 'a')&&(ch <= 'z'))||((ch >= 'A')&&(ch <= 'Z'))){
		printf("%c is an alphabet");
	}else{
		printf("%c is not an alphabet");
	}

	return 0;
}
