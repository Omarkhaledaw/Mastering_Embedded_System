#include<stdio.h>

int main(){

	int array[100],size,i,element;

	printf("Enter no of elements: ");
	fflush(stdin);fflush(stdout);
	scanf("%d",&size);

	for(i = 0;i<size;i++){
		fflush(stdin);fflush(stdout);
		scanf("%d ",&array[i]);
	}
	printf("\nEnter the element to be searched: ");
	fflush(stdin);fflush(stdout);
	scanf("%d",&element);

	for(i = 0;i<size;i++){
		if(){
			printf("Number found at the location = %d",i+1);
			break;
		}else{
			continue;
		}
	}
	return 0;
}
